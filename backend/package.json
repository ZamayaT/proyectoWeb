    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.0",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "supertest": "^7.1.4",
